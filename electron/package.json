{
  "name": "timelog",
  "productName": "timelog",
  "version": "0.0.0",
  "private": true,
  "description": "TimeLog is a desktop application allowing you to track your time",
  "main": "dist/main.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make",
    "publish": "tsc && electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "fluidblue",
    "email": "fluidblue@users.noreply.github.com"
  },
  "license": "GPL-3.0-or-later",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "timelog"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "electron": "17.1.2",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "better-sqlite3": "^7.5.0",
    "electron-squirrel-startup": "^1.0.0"
  }
}
