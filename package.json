{
  "name": "timelog-loader",
  "version": "1.0.0",
  "description": "Loader for timelog",
  "private": true,
  "scripts": {
    "install": "npm run frontend:install && npm run electron:install",
    "start": "npm-run-all -p frontend:start electron:start",
    "make": "npm run frontend:build && npm run electron:make",
    "frontend:install": "npm --prefix \"./frontend\" install",
    "frontend:start": "npm --prefix \"./frontend\" start",
    "frontend:build": "npm --prefix \"./frontend\" run build",
    "electron:install": "npm --prefix \"./electron\" install",
    "electron:start": "npm --prefix \"./electron\" start",
    "electron:make": "npm --prefix \"./electron\" run make"
  },
  "author": "fluidblue",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
  }
}
